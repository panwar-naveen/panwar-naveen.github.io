                 pathToTabImage: 'images/query-bt.png',          //path to the image for the tab *required*
                 imageHeight: '275px',                               //height of tab image *required*
                 imageWidth: '64px',                               //width of tab image *required*    
                 tabLocation: 'right',                               //side of screen where tab lives, top, right, bottom, or left
                 speed: 1000,                                        //speed of animation
                 action: 'click',                                   //options: 'click' or 'hover', action to trigger animationfixedPosition: true,                               //options: true makes it stick(fixed position) on scroll
                 onLoadSlideOut: false,
			
             });
         });
</script>
